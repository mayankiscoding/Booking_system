<%- include("partials/header.ejs") %>

  
  <form action="/seminar/book" method="post" class="form-group">
    <h1>BOOK SEMINAR HALL</h1> 

    <div class="form-row">

      <div class="form-group col-md-6">
        <label for="inputEmail4">Event</label>
        <input type="text" class="form-control" name="act" id="inputEmail4">
      </div>

    </div>

    <div class="form-group">
      <div class="radios hall">
        <h5>Halls</h5>
    
        <input type="radio" name="room" id="audi" value="Auditorium">
        <label for="audi">Auditorium</label>
    
        <input type="radio" name="room" id="404" value="404">
        <label for="404">404</label>
    
        <input type="radio" name="room" id="504" value="504">
        <label for="504">504</label>
    
      </div>
    </div>

    <div class="times form-row date">
      <div class="form-group">
        <label for="date">Date</label>
        <input type="date" class="form-control" name="date" id="date">
      </div>
    </div>



    <button type="submit" class="btn btn-secondary btn-lg btn-dark">Book</button>
  </form>

</body>
</html>